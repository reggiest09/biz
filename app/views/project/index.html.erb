<div class="video-container">
        <iframe width="560" height="315" src="https://www.youtube.com/embed/kO1DiCZC43Q" frameborder="0" allowfullscreen></iframe>
      </div>










<br>
        <div class="row pro-projects">
        <div class="col-lg-12">
            <h4>Featured Project</h4>
        </div>
        
      </div>
    

<div class="row" data-no-turbolink>
<br/>
  
  
  <% @projects.each do |project| %>
    <%= link_to project do %>
      <div class="col s12 m6 l4">

        <div class="card">
          
          <div class="card-image waves-effect waves-block waves-ligh">
            <%= image_tag project.image %>
          </div>
          
          <div class="card-content" style="height: 6em">
            <span class="card-title grey-text text-darken-4">
              <%= project.shortname %>
            </span>
          </div>

        </div>
      </div>
    <% end %>
  <% end %>

</div>

